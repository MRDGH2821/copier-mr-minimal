{
  "allowCompoundWords": true,
  "caseSensitive": false,
  "ignorePaths": [
    "**/.git/**",
    "**/.pnpm-lock.json",
    "**/node_modules/**",
    "**/vscode-extension/**",
    ".vscode",
    ".zed",
    "megalinter-reports",
    "package-lock.json"
  ],
  "language": "en,en-GB",
  "useGitignore": true,
  "version": "0.2",
  "words": [
    "bunx",
    "EDITMSG",
    "filesonly",
    "ggshield",
    "mrdgh",
    "prek",
    "pycache",
    "stefanzweifel",
    "venv"
  ]
}
